package bigData;

import data_producer.DataProducer;
import data_producer.MappedFileProducer;
import jobs.WordCount;

public class Driver {

	public static void main(String[] args) {
		DataProducer producer = new MappedFileProducer("/data/chapter6.txt", 1000000l);
		WordCount wc = new WordCount(producer, 2048, 5000, "wordcount/chapter6.csv");
		wc.execute();
	}

	
}
