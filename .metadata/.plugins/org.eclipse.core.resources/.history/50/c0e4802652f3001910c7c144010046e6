package bigData;

import data_producer.DataProducer;
import data_producer.LineProducer;
import jobs.WordCount;

public class Driver {
	public static void main(String[] args) {
		DataProducer<String> producer = new LineProducer(1024 * 1000);
		WordCount wc = new WordCount(producer, 1, 1, "data/chapter6.txt", "wordcount/chapter6.csv");
		wc.execute();
	}
}